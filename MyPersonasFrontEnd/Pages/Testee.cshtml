@page "{name}"
@model MyPersonasFrontEnd.Pages.TesteeModel
@{

}

<ol class="breadcrumb">
    <li class="breadcrumb-item"><a asp-page="/Tests">Testees (For admin)</a></li>
    <li class="breadcrumb-item active">Testee name: @Model.Testee.Name</li>
</ol>

<h2>@Model.Testee.Name</h2>


<div >
    <h3>
        Testee info:<h3>
            <>Username: @Model.Testee.UserName</>
            <p>Name: @Model.Testee.Name</p>
            <p>Surname: @Model.Testee.Surname</p>
            <p>Date of Birth: @Model.Testee.DOB</p>
</div>

<h3>View tests for this testee</h3>
<div class="row">
    <div class="col-md-5">
        <ul class="list-group">
            @foreach (var test in Model.Testee.Test)
            {
                <li class="list-group-item"><a asp-page="Test" asp-route-id="@test.Id">@test.Type</a></li>
            }
        </ul>
    </div>
</div>